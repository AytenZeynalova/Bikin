@model List<Services>
@{
    ViewData["Title"] = "Index";
}



    <section class="website-with-bikin">

        <div class="container">
            <div class="row">

                <div class="col">
                    <h1>Build Better Websites With Bikin</h1>
                    <h3>
                        We are team of talented designers making websites with Bootstrap
                    </h3>


                    <button type="button" class="btn btn-outline-primary border-0">Get Started</button>

                    <div class="imgdiv">
                        <img src="https://bootstrapmade.com/demo/templates/Bikin/assets/img/hero-img.png" alt="">

                    </div>
                </div>

            </div>

        </div>

    </section>

    <section class="about-us">

